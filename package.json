{
    "name": "my-vscode-theme",
    "displayName": "My Vscode Theme",
    "description": "Your VSCode theme.",
    "version": "0.3.0-beta.1",
    "publisher": "you",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Themes"
    ],
    "scripts": {
        "watch": "nodemon --watch ./preprocessing/preprocessor.py --exec npm run preprocess",
         "preprocess": "python3 ./preprocessing/preprocessor.py"
    },
    "contributes": {
        "themes": [
            {
                "label": "My VSCode Theme",
                "uiTheme": "vs-dark",
                "path": "./theming/my-vscode-theme.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/quatquatt/my-vscode-theme"
    },
    "icon": "icon.png",
    "keywords": [
        "hyper black",
        "hyper term",
        "Hyper Black",
        "hyper term black",
        "hyper"
    ],
    "devDependencies": {
        "nodemon": "^3.1.4"
    }
}
